<div class="group-header-container">
<%= link_to "Main", root_path, class: 'group-main-link'%>
<h1 class="group-header">New Group Form</h2>
 <%= link_to 'Log out', logout_path, class: 'group-logout-link'%>
</div>
  <%= form_for @group do |f| %>
    <% @group.errors.full_messages.each do |error| %>
    <p class="errors">* <%= error %></p>
    <% end %> 
    <div class="new-group-form">    
    <%= f.text_field :name, placeholder: 'Name of group here', class: 'newgroup-input' %>
     
    <%= f.text_field :icon, placeholder: 'Icon URL here', class: 'newgroup-input' %>
    
    <%= f.submit "Create", class: 'newgroup-submit'  %>
    </div>
  <% end %>